            end
        elseif species.Value == "Vampire" then
            naming.TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            if rank then
                rank.TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            end
            if head:FindFirstChild("Title") then
